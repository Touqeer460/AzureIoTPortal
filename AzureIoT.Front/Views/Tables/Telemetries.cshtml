@{
    ViewBag.Title = "Index";
}

    @Html.Partial("_LeftMenu")
    <!-- content -->
            <div class="col-md-10">
                <div class="row">
                    <div class="panel panel-default bootstrap-admin-no-table-panel">
                        <div class="panel-heading">
                            <div class="text-muted bootstrap-admin-box-title">Telemetries</div>
                        </div>
                        <div class="bootstrap-admin-no-table-panel-content bootstrap-admin-panel-content collapse in">
                            <form class="form-horizontal">
                                <fieldset>
                                    <legend>Add New</legend>
                                    <div class="form-group">
                                        <label class="col-lg-1 control-label" for="focusedInput">ID</label>
                                        <div class="col-lg-1">
                                            <input class="form-control" id="focusedInput" type="text" value="0, 1, 2">
                                        </div>
                                        <label class="col-lg-2 control-label" for="focusedInput">Telemetry Name</label>
                                        <div class="col-lg-3">
                                            <input class="form-control" id="focusedInput" type="text" value="Humidity, Air Pressure...">
                                        </div>
                                        <label class="col-lg-1 control-label" for="focusedInput">Unit</label>
                                        <div class="col-lg-2">
                                            <input class="form-control" id="focusedInput" type="text" value="rh, pa...">
                                        </div>
                                    </div>
                                    <button type="submit" class="btn btn-primary">Save changes</button>
                                    <button type="reset" class="btn btn-default">Cancel</button>
                                </fieldset>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <div class="text-muted bootstrap-admin-box-title">Existing Telemetries</div>
                        </div>
                        <div class="bootstrap-admin-panel-content">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Name</th>
                                        <th>Unit</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>1</td>
                                        <td>Humidity</td>
                                        <td>rh</td>
                                        <td><a href="#">Edit</a>&nbsp;|&nbsp;<a href="#">Delete</a></td>
                                    </tr>
                                    <tr>
                                        <td>2</td>
                                        <td>Air Pressure</td>
                                        <td>pa</td>
                                        <td><a href="#">Edit</a>&nbsp;|&nbsp;<a href="#">Delete</a></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>


